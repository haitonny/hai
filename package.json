{
  "name": "vue-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "prebuild": "cross-env NODE_ENV=production yarn run preserve",
    "preserve": "tailwindcss build src/assets/styles/tailwind.css -o public/tailwind.output.css",
    "svg": "vsvg -s src/assets/svg -t src/icons --es6"
  },
  "dependencies": {
    "@vunamhung/arg.js": "^1.4.0",
    "axios": "0.20.0",
    "core-js": "3.6.5",
