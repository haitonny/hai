    "register-service-worker": "^1.7.1",
    "tb-skeleton": "^0.3.6",
    "vue": "2.6.12",
    "vue-router": "3.4.3",
    "vue-svgicon": "3.2.8",
    "vuelidate": "^0.7.5",
    "vuex": "^3.4.0",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/typography": "^0.2.0",
    "@vue/cli-plugin-babel": "4.5.4",
    "@vue/cli-plugin-eslint": "4.5.4",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "4.5.4",
